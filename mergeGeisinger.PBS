cd $PBS_O_WORKDIR

### note added back in header line for CHR1 file

# GeisingerSNPtestResult_CHR1.txt
tail -n +2 GeisingerSNPtestResult_CHR1.txt > headless_GeisingerSNPtestResult_CHR1
