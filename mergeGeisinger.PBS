cut -d , -f 1-2 headless_GeisingerSNPtestResult_CHR1 > startchr1
cut -d , -f 4- headless_GeisingerSNPtestResult_CHR1 > endchr1
sed -i 's/chr//g' chr1
paste -d , startchr1 chr1 endchr1 > rf_GeisingerSNPtestResult_CHR1
head -n 1 GeisingerSNPtestResult_CHR1.txt > GeisingerSNPtestResult_CHR.txt
cat rf_GeisingerSNPtestResult_CHR1 >> GeisingerSNPtestResult_CHR.txt

